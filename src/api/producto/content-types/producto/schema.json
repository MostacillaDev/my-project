{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name_product": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "brand": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "stock": {
      "type": "integer"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "status": {
      "type": "boolean"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "slug": {
      "type": "uid",
      "targetField": "name_product"
    },
    "extra_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "C_S_Receta": {
      "type": "boolean",
      "default": false
    },
    "use_carts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::use-cart.use-cart",
      "inversedBy": "products"
    }
  }
}
